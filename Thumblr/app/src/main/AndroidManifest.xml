<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.henry.thumblr">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TermsActivity" />
        <activity android:name=".LoginFocusedActivity" />
        <activity android:name=".LoginEmailActivity" />
        <activity android:name=".LoginPasswordActivity" />
        <activity android:name=".HomeActivity">
        </activity>
        <activity android:name=".SearchActivity" />
        <activity android:name=".ActiveActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".CreateActivity" />
        <activity android:name=".LikesActivity"></activity>
    </application>

</manifest>